{
  "name": "geese",
  "version": "1.1.2",
  "description": "This File is provided for 'active development' (yarn watch)",
  "watch" : {
    "execute": {
      "patterns": [
        "geese"
      ],
      "extensions": "py"
    }
  },
  "scripts": {
    "execute": "yarn sdist",
    "watch": "npm-watch",
    "build": "yarn wheel && yarn sdist",
    "wheel": "python3 -m build --wheel",
    "sdist": "python3 -m build --sdist",
    "pip": "python -m pip install --upgrade -r ./requirements.txt"
  }
}